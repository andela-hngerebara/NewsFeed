    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "build-css": "^0.2.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
